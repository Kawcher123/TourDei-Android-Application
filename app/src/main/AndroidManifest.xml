<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tourdeiapplication">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Welcome"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TravelBlogs"
            android:label="Travel Blogs"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TravellingPlace"
            android:label="Travelling Places"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Signup"
            android:parentActivityName=".login" />
        <activity
            android:name=".login"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ui.home.Make_aTrip"
            android:label="Make a Trip"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>